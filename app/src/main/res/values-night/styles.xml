<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MainScreenBackgroundColor">
        <item name="android:background">@color/yp_black</item>
    </style>

    <style name="SettingsScreenOptionsTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">@color/yp_white</item>
        <item name="android:textSize">@dimen/settings_options_text_size</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
    </style>

    <style name="SearchScreenEditTextHintColor">
        <item name="android:textColorHint">@color/yp_black</item>
    </style>

    <style name="SearchScreenClearBtnBackground">
        <item name="android:background">@color/yp_white</item>
    </style>
</resources>